#include <iostream>
using namespace std;

class Number {
private:
    int value;

public:
    Number(int v = 0) {
        value = v;
    }

    // Overload binary + operator
    Number operator+(Number obj) {
        Number temp;
        temp.value = value + obj.value;  // Add the two values
        return temp;
    }

    void display() {
        cout << "Value = " << value << endl;
    }
};

int main() {
    Number n1(10), n2(20), n3;

    cout << "Number 1: ";
    n1.display();

    cout << "Number 2: ";
    n2.display();

    // Using overloaded + operator
    n3 = n1 + n2;

    cout << "After n1 + n2: ";
    n3.display();

    return 0;
}

